{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "addify",
  "compatibility_date": "2025-12-01",
  "main": "dist/server/index.mjs",
  "compatibility_flags": ["nodejs_compat"],
  "routes": [
    {
      "pattern": "addify.yizack.com",
      "custom_domain": true
    }
  ],
  "assets": {
    "binding": "ASSETS",
    "directory": "dist/public"
  }
}
